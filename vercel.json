{
  "version": 2,
  "builds": [
    {
      "src": "fretamento_project/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "fretamento_project/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "fretamento_project.settings_vercel"
  },
  "functions": {
    "fretamento_project/wsgi.py": {
      "runtime": "python3.9"
    }
  }
}